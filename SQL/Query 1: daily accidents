SELECT start_date,
       COUNT(*) AS total_accidents,
       ROUND(AVG(duration_minutes),2) AS avg_duration_min
FROM madsc102.usaccidents_volume
GROUP BY start_date
ORDER BY start_date DESC
LIMIT 100;


